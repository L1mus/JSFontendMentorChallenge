{
  "name": "tip-calculator-app-main",
  "version": "1.0.0",
  "description": "![Design preview for the Tip calculator app coding challenge](./preview.jpg)",
  "main": "index.js",
  "scripts": {
    "compile:sass": "sass src/scss/main.scss dist/style.css",
    "prefix:css": "postcss dist/style.css --use autoprefixer -o dist/style.css",
    "build": "npm-run-all compile:sass prefix:css",
    "watch:sass": "sass --watch src/scss/main.scss dist/style.css",
    "serve": "live-server",
    "start": "npm-run-all --parallel watch:sass serve"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "sass": "^1.93.3"
  }
}
